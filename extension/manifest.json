{
  "name": "HaveToDo",
  "version": "1.0.0",
  "manifest_version": 2,
  "short_name": "HaveToDo",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "Easy way to manage list of Todos",
  "permissions": [
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "author": "Dhiraj Bodicherla",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "offline_enabled": true,
  "options_page": "options.html",
  "incognito": "split",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}